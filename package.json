{
  "$schema": "https://json.schemastore.org/package",
  "name": "@skilbjo/logger",
  "version": "0.0.1",
  "main": "index.js",
  "typings": "index.d.ts",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skilbjo/logger.git"
  },
  "scripts": {
    "prepare": "if [ -z \"$GITHUB_ACTIONS\" ]; then npx husky install; fi",
    "eslint": "eslint .",
    "depcheck": "depcheck",
    "lint": "npm run eslint",
    "test": "jest --runInBand",
    "build": "tspc -p tsconfig.build.json",
    "postbuild": "jq 'del(.files)' package.json >dist/package.json",
    "prepare-npm": "jq 'del(.publishConfig, .files) | . + { publishConfig: { registry: \"https://registry.npmjs.org/\", access: \"public\", provenance: true } }' package.json >package2.json && mv package2.json dist/package.json"
  },
  "files": [
    "dist"
  ],
  "license": "ISC",
  "author": "skilbjo",
  "keywords": [
    "api",
    "sdk",
    "openapi"
  ],
  "dependencies": {
    "pino": "9.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-angular": "17.0.0",
    "@skilbjo/config-rc": "1.0.29",
    "@skilbjo/eslint-config-rc": "npm:@skilbjo/config-rc@1.0.27",
    "@types/http-errors": "2.0.5",
    "depcheck": "1.4.7",
    "eslint": "8.57.0",
    "http-errors": "2.0.0",
    "husky": "8.0.1",
    "jest": "30.2.0",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "ts-patch": "3.3.0",
    "typescript": "5.9.2",
    "typescript-transform-paths": "3.5.5",
    "winston": "3.18.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
