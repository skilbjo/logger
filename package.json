{
  "$schema": "https://json.schemastore.org/package",
  "name": "@skilbjo/logger",
  "version": "0.0.1",
  "repository": "github:skilbjo/logger.git",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prepare": "if [ -z \"$GITHUB_ACTIONS\" ]; then npx husky install; fi",
    "eslint": "eslint .",
    "depcheck": "depcheck",
    "lint": "npm run eslint",
    "test": "jest --runInBand",
    "build": "tspc -p tsconfig.build.json",
    "postbuild": "jq 'del(.files)' package.json >dist/package.json",
    "prepare-npm": "jq 'del(.repository, .publishConfig, .files) | . + { publishConfig: { registry: \"https://registry.npmjs.org/\" } }' package.json >package2.json && mv package2.json dist/package.json"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/skilbjo"
  },
  "files": [
    "dist"
  ],
  "license": "ISC",
  "author": "skilbjo",
  "keywords": [
    "api",
    "sdk",
    "openapi"
  ],
  "dependencies": {
    "pino": "9.4.0",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-angular": "17.0.0",
    "@skilbjo/config-rc": "1.0.12",
    "@skilbjo/eslint-config-rc": "npm:@skilbjo/config-rc@1.0.9",
    "@types/http-errors": "2.0.4",
    "depcheck": "1.4.7",
    "eslint": "8.57.0",
    "http-errors": "2.0.0",
    "husky": "8.0.1",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "ts-patch": "3.2.1",
    "typescript": "5.5.4",
    "typescript-transform-paths": "3.5.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
